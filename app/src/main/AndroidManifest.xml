<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.resrrecycleview">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.StudentDataActivity"
                    android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".service.SetUpdateDbIntentService"
            android:exported="false"/>
        <service android:name=".service.SetUpdateService"
            android:exported="false"/>
        <receiver android:name=".fragment.AddUpdateFragment$StudentBroadcastReceiver" />
    </application>

</manifest>